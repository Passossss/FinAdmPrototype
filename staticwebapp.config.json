{
  "routes": [
    {
      "route": "/assets/*",
      "serve": "/assets/*",
      "statusCode": 200
    },
    {
      "route": "/*.js",
      "serve": "/*.js",
      "statusCode": 200
    },
    {
      "route": "/*.css",
      "serve": "/*.css",
      "statusCode": 200
    },
    {
      "route": "*.png",
      "serve": "*.png",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".css": "text/css",
    ".png": "image/png",
    ".svg": "image/svg+xml"
  }
}
