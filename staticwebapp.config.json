{
  "routes": [
    {
      "route": "/assets/*",
      "serve": "/assets/*"
    },
    {
      "route": "*.json",
      "serve": "*.json"
    },
    {
      "route": "*.js",
      "serve": "*.js"
    },
    {
      "route": "*.css",
      "serve": "*.css"
    },
    {
      "route": "*.png",
      "serve": "*.png"
    },
    {
      "route": "*.svg",
      "serve": "*.svg"
    },
    {
      "route": "*.ico",
      "serve": "*.ico"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".css": "text/css",
    ".png": "image/png",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon"
  }
}
